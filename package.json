{
	"name": "emotionawareide",
	"displayName": "EmotionAwareIDE",
	"description": "Predicts user mood and gives tips based on input from some equipment",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emotionawareide.start_dashboard",
				"title": "Show Dashboard"
			},
			{
				"command": "emotionawareide.show_message",
				"title": "Show message"
			}
		],
		"configuration": {
			"title": "EmotionAwareIDE",
			"properties": {
				"emotionawareide.port": {
					"type": "number",
					"default": 1337,
					"description": "The port used for receiving data."
				}
			}	
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	}
}
