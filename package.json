{
	"name": "emotionawareide",
	"displayName": "EmotionAwareIDE",
	"description": "Predicts user mood and gives tips based on input from some equipment",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "emotionawareide.start_dashboard",
				"title": "Show Dashboard"
			},
			{
				"command": "emotionawareide.show_message",
				"title": "Show message"
			},
			{
				"command": "emotionawareide.start_survey",
				"title": "Start Survey"
			}
		],
		"configuration": [
			{
			"id" : "EmotionAwareIDE",
			"title": "EmotionAwareIDE"
			},
			{
				"title" : "Server",
				"properties": {
					"emotionawareide.server.port": {
						"order" : 2,
						"type": "integer",
						"default": 1337,
						"description": "The port used for receiving data."
					}
				}
			},
			{
				"title":"E4",
				"description" : "Description",
				"properties": {
					"emotionawareide.e4.port" : {
						"type": "integer",
						"default": 8080,
						"description": "The port used for transmission of E4 data."
					},
					"emotionawareide.e4.wristbands" : {
						"type" : "string",
						"default" : "addr1",
						"enum" : ["addr1", "addr2"],
						"enumDescriptions" : [
							"Chose to connect to the E4 wristband with this address.",
							"Chose to connect to the E4 wristband with this address."
						]
					}
				}
			},
			{
				"title":"Eye Tracker",
				"properties": {
					"emotionawareide.eye.port" : {
						"type": "integer",
						"default": 4242,
						"description": "The port used for transmission of Eyetracker data."
					}
				}
			},
			{
				"title" : "Actions",
				"properties": {
					"emotionawareide.action.baseActions" : {
						"type" : "object"
					}
				}
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	}
}
